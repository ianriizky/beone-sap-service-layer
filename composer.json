{
    "name": "ianriizky/beone-sap-service-layer",
    "description": "The bridging between your application and BeOne SAP Service Layer using Laravel HTTP Client.",
    "type": "library",
    "keywords": [
        "php",
        "laravel",
        "beone",
        "sap",
        "service layer"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ianriizky/beone-sap-service-layer/issues",
        "source": "https://github.com/ianriizky/beone-sap-service-layer"
    },
    "authors": [
        {
            "name": "Septianata Rizky Pratama",
            "email": "septianata.pratama@beone-solution.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^8.83.12|^9.12",
        "illuminate/http": "^8.83.12|^9.12",
        "illuminate/support": "^8.83.12|^9.12"
    },
    "require-dev": {
        "illuminate/testing": "^8.83.12|^9.12",
        "orchestra/testbench": "^6.24|^7.3",
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Ianriizky\\BeoneSAPServiceLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ianriizky\\BeoneSAPServiceLayer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ianriizky\\BeoneSAPServiceLayer\\ServiceProvider"
            ],
            "aliases": {
                "SAPServiceLayer": "Ianriizky\\BeoneSAPServiceLayer\\Support\\Facades\\SAPServiceLayer"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
